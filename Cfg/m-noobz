USERNAME : <code>$user</code>
PASSWORD : <code>$pass</code>
IP LIMIT : $ip
EXP DAYS : $expi
tcp_std port : 8080-8880
tcp_ssl port : 8443-443
◇━━━━━━━━━━━━━━━━━◇
       TCP STD 8080
◇━━━━━━━━━━━━━━━━━◇

