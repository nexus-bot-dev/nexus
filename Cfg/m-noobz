read -n 1 -s -r -p "Press any key to back on menu"
menu
}


function delete(){
clear
echo -e "$BLUE╭═══════════════════════════════════════════════════╮${NC}"
echo -e "$BLUE│ ${NC}${LIGHT}               ${LIGHT}• DELETE NOOBZ •                   ${NC}$BLUE │ $NC"
echo -e "$BLUE╰═══════════════════════════════════════════════════╯${NC}"

NUMBER_OF_CLIENTS=$(grep -c -E "^### " "/etc/xray/noob")
	if [[ ${NUMBER_OF_CLIENTS} == '0' ]]; then
		echo ""
		echo "You have no existing clients!"
		exit 1
	fi

	echo ""
	echo " Select the existing client you want to remove"
	echo " Press CTRL+C to return"
	echo " ==============================="
	echo "     No  Expired   User"
	grep -E "^### " "/etc/xray/noob" | cut -d ' ' -f 2-3 | nl -s ') '
	until [[ ${CLIENT_NUMBER} -ge 1 && ${CLIENT_NUMBER} -le ${NUMBER_OF_CLIENTS} ]]; do
		if [[ ${CLIENT_NUMBER} == '1' ]]; then
			read -rp "Select one client [1]: " CLIENT_NUMBER
		else
			read -rp "Select one client [1-${NUMBER_OF_CLIENTS}]: " CLIENT_NUMBER
		fi
	done
CLIENT_NAME=$(grep -E "^### " "/etc/xray/noob" | cut -d ' ' -f 2-3 | sed -n "${CLIENT_NUMBER}"p)
user=$(grep -E "^### " "/etc/xray/noob" | cut -d ' ' -f 2 | sed -n "${CLIENT_NUMBER}"p)
exp=$(grep -E "^### " "/etc/xray/noob" | cut -d ' ' -f 3 | sed -n "${CLIENT_NUMBER}"p)
sed -i "/^### $user $exp/d" /etc/xray/noob
sed -i '/^,"'"$user"'"$/d' /etc/noobzvpns/users.json
noobzvpns --remove-user $user
expi=`date -d "$exp days" +"%Y-%m-%d"`
systemctl restart noobzvpns

clear
echo -e "$BLUE╭═══════════════════════════════════════════════════╮${NC}"
echo -e "$BLUE│ ${NC}${LIGHT}              ${LIGHT}• DELETE NOOBZ •                   ${NC}$BLUE │ $NC"
echo -e "$BLUE╰═══════════════════════════════════════════════════╯${NC}"
echo -e ""
echo -e "$LIME USERNAME $user HAS BEEN DELETE $NC"
echo -e "$LIME EXPIRED : $expi $NC"
echo -e ""
TEXT="
<code>◇━━━━━━━━━━━━━━◇</code>
<code>  DELETE NOOBZVPNS</code>
<code>◇━━━━━━━━━━━━━━◇</code>
<code>DOMAIN   :</code> <code>${domain} </code>
<code>ISP      :</code> <code>$ISP $CITY </code>
<code>USERNAME :</code> <code>$user</code>
<code>EXPIRED  :</code> <code>$expi </code>
<code>◇━━━━━━━━━━━━━━◇</code>
<i>Succes Delete This User...</i>
"
curl -s --max-time $TIMES -d "chat_id=$CHATIDNEW&disable_web_page_preview=1&text=$TEXT&parse_mode=html" $URL >/dev/null
cd
if [ ! -e /etc/tele ]; then
echo -ne
else
echo "$TEXT" > /etc/notiftele
bash /etc/tele
fi
echo -e "$BLUE╭══════════════════════ • ${LIME}BY${NC}${BLUE} • ═══════════════════════╮${NC}"
echo -e "$BLUE${NC}           ${LIGHT}            • JABAR TUNNELING SCRIPT •                 $BLUE $NC"
echo -e "$BLUE╰═════════════════════════════════════════════════════╯${NC}"
read -n 1 -s -r -p "Press any key to back on menu"
m-noobz
}


function renew(){
clear
echo -e "$BLUE╭═══════════════════════════════════════════════════╮${NC}"
echo -e "$BLUE│ ${NC}${LIGHT}              ${LIGHT}• RENEW NOOBZ •                    ${NC}$BLUE │ $NC"
echo -e "$BLUE╰═══════════════════════════════════════════════════╯${NC}"
read -p "username: " user
read -p "expired?: " exp
noobzvpns --renew-user $user --expired-user $user $exp
clear
echo -e "$BLUE╭═══════════════════════════════════════════════════╮${NC}"
echo -e "$BLUE│ ${NC}${LIGHT}              ${LIGHT}• RENEW NOOBZ •                    ${NC}$BLUE │ $NC"
echo -e "$BLUE╰═══════════════════════════════════════════════════╯${NC}"
echo "USERNAME: $user"
echo "renew success!!"
echo -e "$BLUE╭══════════════════════ • ${LIME}BY${NC}${BLUE} • ═══════════════════════╮${NC}"
echo -e "$BLUE${NC}          ${LIGHT}   • JABAR TUNNELING SCRIPT •                 $BLUE $NC"
echo -e "$BLUE╰═════════════════════════════════════════════════════╯${NC}"
read -n 1 -s -r -p "Press any key to back on menu"
menu
}




function lock(){
clear
echo -e "$BLUE╭═══════════════════════════════════════════════════╮${NC}"
echo -e "$BLUE│ ${NC}${LIGHT}              ${LIGHT}• LOCK  NOOBZ •                    ${NC}$BLUE │ $NC"
echo -e "$BLUE╰═══════════════════════════════════════════════════╯${NC}"
read -p "username: " user
noobzvpns --block-user $user
clear
echo -e "$BLUE╭═══════════════════════════════════════════════════╮${NC}"
echo -e "$BLUE│ ${NC}${LIGHT}              ${LIGHT}• LOCK  NOOBZ •                    ${NC}$BLUE │ $NC"
echo -e "$BLUE╰═══════════════════════════════════════════════════╯${NC}"
echo "USERNAME: $user"
echo "locked success!!"
echo -e "$BLUE╭══════════════════════ • ${LIME}BY${NC}${BLUE} • ═══════════════════════╮${NC}"
echo -e "$BLUE${NC}          ${LIGHT}   • JABAR TUNNELING SCRIPT •                 $BLUE $NC"
echo -e "$BLUE╰═════════════════════════════════════════════════════╯${NC}"
read -n 1 -s -r -p "Press any key to back on menu"
m-noobz
}






function show(){

clear
echo -e "$BLUE╭═══════════════════════════════════════════════════╮${NC}"
echo -e "$BLUE│ ${NC}${LIGHT}             ${LIGHT}• MEMBER  NOOBZ •                   ${NC}$BLUE │ $NC"
echo -e "$BLUE╰═══════════════════════════════════════════════════╯${NC}"
noobzvpns --info-all-user | awk '/^\s*\+.*-> active/ {gsub(/(issued\(yyyymmdd\)|hash_key): [0-9a-f]+/, ""); print; getline; print; getline; getline; getline; print; print "═══════════════════"}'

echo -e "$BLUE╭══════════════════════ • ${LIME}BY${NC}${BLUE} • ═══════════════════════╮${NC}"
echo -e "$BLUE${NC}          ${LIGHT}   • JABAR TUNNELING SCRIPT •                 $BLUE $NC"
echo -e "$BLUE╰═════════════════════════════════════════════════════╯${NC}"
read -n 1 -s -r -p "Press any key to back on menu"
m-noobz

}



function unlock(){
clear
echo -e "$BLUE╭═══════════════════════════════════════════════════╮${NC}"
echo -e "$BLUE│ ${NC}${LIGHT}             ${LIGHT}• UNLOCK  NOOBZ •                   ${NC}$BLUE │ $NC"
echo -e "$BLUE╰═══════════════════════════════════════════════════╯${NC}"
read -p "username: " user
noobzvpns --unblock-user $user
clear
echo -e "$BLUE╭═══════════════════════════════════════════════════╮${NC}"
echo -e "$BLUE│ ${NC}${LIGHT}             ${LIGHT}• UNLOCK  NOOBZ •                   ${NC}$BLUE │ $NC"
echo -e "$BLUE╰═══════════════════════════════════════════════════╯${NC}"
echo "USERNAME: $user"
echo "unlocked success!!"
echo -e "$BLUE╭══════════════════════ • ${LIME}BY${NC}${BLUE} • ═══════════════════════╮${NC}"
echo -e "$BLUE${NC}          ${LIGHT}   • JABAR TUNNELING SCRIPT •                 $BLUE $NC"
echo -e "$BLUE╰═════════════════════════════════════════════════════╯${NC}"
read -n 1 -s -r -p "Press any key to back on menu"
m-noobz
}



function remove(){
clear
echo -e "$BLUE╭═══════════════════════════════════════════════════╮${NC}"
echo -e "$BLUE│ ${NC}${LIGHT}             ${LIGHT}• REMOVE  NOOBZ •                   ${NC}$BLUE │ $NC"
echo -e "$BLUE╰═══════════════════════════════════════════════════╯${NC}"

read -p "Apakah Anda ingin menghapus semua user? (Y/N): " choice

if [ "$choice" == "Y" ] || [ "$choice" == "y" ]; then
    # Eksekusi perintah di sini
    echo "MENGHAPUS SEMUA USER!"
noobzvpns --remove-all-user
elif [ "$choice" == "N" ] || [ "$choice" == "n" ]; then
    echo "Membatalkan penghapusan."
menu-noobzvpns
else
    echo "Pilihan tidak valid."
fi

clear
echo -e "$BLUE╭═══════════════════════════════════════════════════╮${NC}"
echo -e "$BLUE│ ${NC}${LIGHT}             ${LIGHT}• REMOVE  NOOBZ •                   ${NC}$BLUE │ $NC"
echo -e "$BLUE╰═══════════════════════════════════════════════════╯${NC}"
echo "USERNAME: $user"
echo "unlocked success!!"
echo -e "$BLUE╭══════════════════════ • ${LIGHT}BY${NC}${BLUE} • ═══════════════════════╮${NC}"
echo -e "$BLUE${NC}          ${LIGHT}   • JABAR TUNNELING SCRIPT •                 $BLUE $NC"
echo -e "$BLUE╰═════════════════════════════════════════════════════╯${NC}"
read -n 1 -s -r -p "Press any key to back on menu"
m-noobz
}




clear
echo -e "$BLUE┌───────────────────────────────────────────────────┐${NC}"
echo -e "$BLUE│${NC} \033[44;1;97;1m              • NOOBZ PANEL MENU •               ${NC}$BLUE │$NC"
echo -e "$BLUE╰───────────────────────────────────────────────────┘${NC}"
echo -e "$BLUE┌───────────────────────────────────────────────────┐${NC}"
echo -e "$BLUE│  $NC ${YELLOW}[${YELLOW}1${YELLOW}]${NC} ${CYAN}☞ ${NC} ${LIGHT}ADD AKUN${NC}       ${YELLOW}[${YELLOW}4${YELLOW}]${NC} ${CYAN}☞ ${NC} ${LIGHT}DELETE USER${NC}  $LIME $NC"
echo -e "$BLUE│  $NC ${YELLOW}[${YELLOW}2${YELLOW}]${NC} ${CYAN}☞ ${NC} ${LIGHT}SHOW AKUN${NC}      ${YELLOW}[${YELLOW}5${YELLOW}]${NC} ${CYAN}☞ ${NC} ${LIGHT}LOCKED USER${NC}   $LIME $NC"
echo -e "$BLUE│  $NC ${YELLOW}[${YELLOW}3${YELLOW}]${NC} ${CYAN}☞ ${NC} ${LIGHT}RENEW AKUN${NC}     ${YELLOW}[${YELLOW}6${YELLOW}]${NC} ${CYAN}☞ ${NC} ${LIGHT}UNLOCKED USER${NC}   $LIME $NC"
echo -e "$BLUE│  $NC ${YELLOW}[${YELLOW}x${YELLOW}]${NC} ${CYAN}☞ ${NC} ${LIGHT}GO BACK${NC}        ${YELLOW}[${YELLOW}7${YELLOW}]${NC} ${CYAN}☞ ${NC} ${LIGHT}REMOVE ALL USER${NC}   $LIME $NC"
echo -e "$BLUE╰───────────────────────────────────────────────────┘${NC}"
echo -e "$BLUE┌───────────────────────────────────────────────────┐${NC}"
echo -e "$BLUE│${NC} ${LIGHT}            • JABAR TUNNELING SCRIPT •          ${NC}$BLUE  │$NC"
echo -e "$BLUE╰───────────────────────────────────────────────────┘${NC}"
echo -e ""
echo -ne " ${LIME}Select menu ${LIME}: ${LIME}"; read opt
case $opt in
01 | 1) clear ; create ;;
02 | 2) clear ; show ;;
03 | 3) clear ; renew ;;
04 | 4) clear ; delete ;;
05 | 5) clear ; lock ;;
06 | 6) clear ; unlock ;;
07 | 7) clear ; remove ;;
100) clear ; $up2u ;;
x | 0) clear ; menu ;;
*) clear ; menu ;;
